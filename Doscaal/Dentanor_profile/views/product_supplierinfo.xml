<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="product_supplierinfo_search_view" model="ir.ui.view">
        <field name="name">Product supplierinfo search</field>
        <field name="model">product.supplierinfo</field>
        <field eval="1" name="priority"/>
        <field name="inherit_id" ref="product.product_supplierinfo_search_view"/>
        <field name="arch" type="xml">
            <xpath expr="//filter[@name='archived']" position="after">
                <filter string="Produit inactif" name="archived_product" domain="[('product_tmpl_id.active', '=', False)]"/>
            </xpath>
        </field>
    </record>

</odoo>
